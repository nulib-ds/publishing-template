	journal = {Journal of Management Studies},
	author = {Delbridge, Rick},
	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/joms.12052},
	keywords = {CMS, critical management studies, impact, post-disciplinarity, public value, social science},
	pages = {95--117},
	file = {Full Text PDF:C\:\\Users\\cpd3149\\Zotero\\storage\\3IJYKYQ3\\Delbridge - 2014 - Promising Futures CMS, Post-Disciplinarity, and t.pdf:application/pdf;Snapshot:C\:\\Users\\cpd3149\\Zotero\\storage\\6I8H26GL\\joms.html:text/html},
