	year = {2016},
	note = {Publisher: IFE Centre for Psychological Studies (ICPS)},
	keywords = {Addictive behaviour, ASEAN, Health disparities, Mental health, Sexual minority, University students},
	pages = {7806--7815},
}

@article{pflum_social_2015,
