	keywords = {Body dissatisfaction, Body image, Disordered eating, Gender identity},
	pages = {118--125},
	file = {Full Text PDF:/Users/delaneynelson/Zotero/storage/PLQVK273/Ålgars et al. - 2010 - Conflicted Gender Identity, Body Dissatisfaction, .pdf:application/pdf},
}

