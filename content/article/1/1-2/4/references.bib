	keywords = {netnography, nutrition, qualitative research, social media, transgender, video blogs},
	pages = {340--349},
	file = {Full Text PDF:/Users/delaneynelson/Zotero/storage/2U8LXNVV/Schier and Linsenmeyer - 2019 - Nutrition-Related Messages Shared Among the Online.pdf:application/pdf},
}

