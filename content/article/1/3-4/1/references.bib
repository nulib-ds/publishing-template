	number = {8},
	urldate = {2023-01-05},
	journal = {Archives of Sexual Behavior},
	author = {Reisner, Sari L. and Biello, Katie and Rosenberger, Joshua G. and Austin, S. Bryn and Haneuse, Sebastien and Perez-Brumer, Amaya and Novak, David S. and Mimiaga, Matthew J.},
	month = nov,
