	keywords = {cisnormativity, Gender, gender identity, research methods, transgender},
	pages = {332--344},
	file = {Full Text PDF:/Users/delaneynelson/Zotero/storage/5TG8GWLA/Lindqvist et al. - 2021 - What is gender, anyway a review of the options fo.pdf:application/pdf},
}

