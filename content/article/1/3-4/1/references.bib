	urldate = {2023-01-04},
	journal = {Psychology \& Sexuality},
	author = {Lindqvist, Anna and Sendén, Marie Gustafsson and Renström, Emma A.},
	month = oct,
	year = {2021},
	note = {Publisher: Routledge
