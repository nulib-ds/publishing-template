\_eprint: https://doi.org/10.1080/19419899.2011.576696},
	keywords = {‘gender identity disorder’, cisgenderism, content analysis, misgendering, pathologising, transgender, transsexual},
	pages = {137--160},
	file = {Submitted Version:/Users/delaneynelson/Zotero/storage/SUD5YD75/Ansara and Hegarty - 2012 - Cisgenderism in psychology pathologising and misg.pdf:application/pdf},
