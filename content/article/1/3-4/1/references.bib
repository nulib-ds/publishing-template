	file = {SAGE PDF Full Text:/Users/delaneynelson/Zotero/storage/CBTGZLA9/Linstead and Pullen - 2006 - Gender as multiplicity Desire, displacement, diff.pdf:application/pdf},
}

@article{lipsky_impact_2012,
	title = {Impact of {Sexual} {Orientation} and {Co}-{Occurring} {Disorders} on {Chemical} {Dependency} {Treatment} {Outcomes}},
