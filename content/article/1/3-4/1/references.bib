	keywords = {Gender identity, Health, HIV, Surveillance, Transgender},
	pages = {1503--1514},
	file = {Accepted Version:/Users/delaneynelson/Zotero/storage/IXXTIGXE/Reisner et al. - 2014 - Using a Two-Step Method to Measure Transgender Ide.pdf:application/pdf},
