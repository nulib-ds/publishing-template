	file = {Accepted Version:/Users/delaneynelson/Zotero/storage/HNIYQYTW/Reisner et al. - 2016 - Advancing methods for US transgender health resear.pdf:application/pdf;Snapshot:/Users/delaneynelson/Zotero/storage/IFSWBL9N/Advancing_methods_for_US_transgender_health.18.html:text/html},
